 {
  "name": "Deflaaate",
  "version": "0.1.1",
  "description": "Crap comment remover for www.dribbble.com",
  "background": {"page": "background.html"},
  "manifest_version": 2,
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "js": ["background.js"],
    "css": ["customStyles.css"],
    "matches": [ "http://dribbble.com/*", "https://dribbble.com/*"]
  }]
}